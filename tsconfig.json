{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "es6", "dom"],
      "types": ["cypress", "node"],
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["cypress/**/*.ts"]
  }